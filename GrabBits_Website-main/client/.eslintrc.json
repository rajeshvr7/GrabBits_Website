{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaFeature": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-unused-vars": 1,
		"no-undef": 1,
		"react/prop-types": 0
	}
}
